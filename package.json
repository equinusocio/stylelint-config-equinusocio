{
  "name": "stylelint-config-equinusocio",
  "version": "1.6.8",
  "description": "Stylelint configuration based on my experience and taste",
  "main": "index.js",
  "publishConfig": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/equinusocio/stylelint-config-equinusocio"
  },
  "keywords": [
    "stylelint",
    "stylelint-config",
    "styelint-config-equinusocio",
    "stylelint-config-standard",
    "Mattia Astorino",
    "Equinusocio",
    "standard"
  ],
  "author": "Mattia Astorino",
  "license": "MIT",
  "scripts": {
    "lint": "eslint .",
    "pub": "cross-var pnpm publish --access public",
    "release:major": "git pull && standard-version --release-as major && git push --follow-tags",
    "release:minor": "git pull && standard-version --release-as minor && git push --follow-tags",
    "release:patch": "git pull && standard-version --release-as patch && git push --follow-tags",
    "update:deps": "npx npm-upgrade"
  },
  "peerDependencies": {
    "stylelint-config-standard": ">=33.0.0",
    "stylelint-gamut": ">=1.3.1"
  },
  "devDependencies": {
    "@lasalefamine/eslint-config-base": "3.0.2",
    "@typescript-eslint/eslint-plugin": "5.59.8",
    "@typescript-eslint/parser": "5.59.8",
    "cross-var": "1.1.0",
    "eslint": "8.41.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "standard-version": "9.5.0",
    "stylelint": "15.6.2",
    "stylelint-config-standard": ">=33.0.0",
    "stylelint-gamut": "1.3.1",
    "typescript": "5.0.4"
  }
}
